{
    "extends": [
      "standard"
    ],
    "plugins": [
      "import",
      "standard"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "space-before-function-paren": 0,
      "new-cap": 0
    }
}
